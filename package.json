    "last 2 Opera versions",
    "last 2 Edge versions"
  ],
  "devDependencies": {
    "autoprefixer": "^9.8.6",
    "eslint": "^7.21.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-react": "^7.22.0",
    "fs-extra": "^9.1.0",
    "ghp": "^1.6.0",
    "kleur": "^4.1.4",
    "sass": "^1.32.8",
    "stylelint-order": "^4.1.0",
    "stylelint-scss": "^3.19.0",
    "vite": "^2.1.5"
  },
  "dependencies": {
    "dlv": "^1.1.3",
    "dset": "^3.1.0"
