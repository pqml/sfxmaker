{
  "private": true,
  "name": "sfxmaker",
  "sideEffects": "*.scss",
  "version": "0.0.1",
  "engines": {
    "node": ">=15"
  },
  "scripts": {
    "start": "node node/dev",
    "build": "node node/build",
    "deploy": "ghp dist"
  },
  "author": "Merci-Michel",
  "license": "MIT",
  "eslintConfig": {
    "extends": "./.vscode/.eslintrc.js"
  },
  "stylelint": {
    "extends": "./.vscode/.stylelintrc.js"
  },
  "browserslist": [
    "> 0.5%",
    "not ie > 0",
    "not ie_mob > 0",
    "not Safari < 12.2",
    "not iOS < 13",
    "not Edge < 80",
